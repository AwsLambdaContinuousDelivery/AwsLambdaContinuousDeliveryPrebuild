                                },
                                {
                                    "Action": [
                                        "iam:DeleteRole",
                                        "iam:DeleteRolePolicy",
                                        "iam:GetRole",
                                        "iam:PutRolePolicy",
                                        "iam:CreateRole",
                                        "iam:PassRole"
